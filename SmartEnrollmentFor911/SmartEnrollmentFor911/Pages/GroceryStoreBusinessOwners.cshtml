@page
@model SmartEnrollmentFor911.Pages.GroceryStoreBusinessOwnersModel
@{
    ViewData["Title"] = "GroceryStoreBusinessOwners";
    var businessOwnerList = (StoreBusinessOwners.GroceryStoreBusinessOwnerAPI[])ViewData["BusinessOwners"];
}

<h1>Grocery Store Business Owners</h1>
<h4>Note: Below data is take from fellow groups API service</h4>
<table class="table table-striped table-hover">
    <thead class="thead-light">
        <tr>
            <th>Legal Name</th>
            <th>License Description</th>
            <th>Business Activity</th>
            <th>License Status</th>
            <th>Zip Code</th>
        </tr>
    </thead>
    <tbody>
        @foreach (StoreBusinessOwners.GroceryStoreBusinessOwnerAPI owner in businessOwnerList)
        {
            <tr>
                <td>@owner.LegalName</td>
                <td>@owner.LicenseDescription</td>
                <td>@owner.BusinessActivity</td>
                <td>@owner.LicenseStatus</td>
                <td>@owner.ZipCode</td>
            </tr>
        }

    </tbody>
</table>