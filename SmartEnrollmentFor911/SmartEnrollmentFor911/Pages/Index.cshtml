@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var enrollist = (List<QuickType911Enrollment.Smart911Enrollment>)ViewData["Enrollist"];
    QuickType911Enrollment.Smart911Enrollment[] smart911Enrollments = (QuickType911Enrollment.Smart911Enrollment[])ViewData["Smart911Enrollments"];
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<p>Smart911 Enrollments details in the areas where crimes are reported.</p>

<head>
    <style type="text/css" media="screen">

        table {
            border-collapse: collapse;
            border: 1px solid #000000;
        }

        table td 
        {
            border: 1px solid #000000;
        }

        table tr 
        {
            border: 1px solid #ffffff;
        }
    </style>
</head>
<table class="table table-striped table-hover">
    <thead class="thead-dark">
        <tr>
            <th>Zip</th>
            <th>Web Enrollments</th>
            <th>App Enrollments</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        @foreach (QuickType911Enrollment.Smart911Enrollment enroll in enrollist)
        {
            <tr>
                <td>@enroll.ZipCode</td>
                <td>@enroll.WebEnrollments</td>
                <td>@enroll.AppEnrollments</td>
                <td>@enroll.TotalEnrollments</td>

            </tr>
        }
    </tbody>
</table>